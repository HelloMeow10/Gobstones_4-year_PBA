program{//Lineas negras con procedimientos. Ult Modificacion:25-08-2022
    Lineanegrahorizontal() 
    Lineanegravertical()
    Lineados()
    Lineatres()
}
procedure Lineanegravertical(){
    repeat(4){
        Poner(Negro)
        Mover(Norte)}
}
procedure Lineanegrahorizontal(){
    repeat(9){
        Poner(Negro)
        Mover(Este)}
}
procedure Lineados(){
    repeat(9){
        Poner(Negro)
        Mover(Oeste)}
}
procedure Lineatres(){
    repeat(4){
        Poner(Negro)
        Mover(Sur)
    }
}